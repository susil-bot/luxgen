{
  "name": "trainer-platform",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/three": "^0.178.1",
    "axios": "^1.12.2",
    "gsap": "^3.13.0",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.17",
    "three": "^0.178.0",
    "typescript": "^4.9.5",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.24.0",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "cypress": "^13.15.0",
    "cypress-mochawesome-reporter": "^3.6.0",
    "cypress-real-events": "^1.10.0",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.5.6",
    "serve": "^14.2.1",
    "tailwind-merge": "^2.6.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:optimized": "./scripts/build.sh",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "quality": "npm run lint && npm run type-check && npm run test:coverage",
    "preview": "vercel --prod",
    "deploy": "vercel --prod",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "deploy:force": "./scripts/deploy.sh production true",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf build dist node_modules/.cache",
    "setup": "npm ci && npm run type-check",
    "dev": "npm start",
    "prod": "npm run build:optimized && npm run deploy:production",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "test:api": "cypress run --spec 'cypress/e2e/api-endpoints.cy.ts'",
    "test:api:headed": "cypress run --spec 'cypress/e2e/api-endpoints.cy.ts' --headed",
    "test:api:chrome": "cypress run --spec 'cypress/e2e/api-endpoints.cy.ts' --browser chrome",
    "test:api:firefox": "cypress run --spec 'cypress/e2e/api-endpoints.cy.ts' --browser firefox",
    "test:api:edge": "cypress run --spec 'cypress/e2e/api-endpoints.cy.ts' --browser edge",
    "test:api:all": "npm run test:api:chrome && npm run test:api:firefox && npm run test:api:edge",
    "test:api:report": "npm run test:api && node scripts/generate-test-report.js",
    "test:api:ci": "cypress run --spec 'cypress/e2e/api-endpoints.cy.ts' --browser chrome --headless --reporter json --reporter-options output=cypress/results/results.json",
    "test:api:full": "npm run test:api:ci && node scripts/generate-test-report.js cypress/results/results.json",
    "test:api:local": "API_BASE_URL=http://localhost:3000 npm run test:api",
    "test:api:staging": "API_BASE_URL=https://staging-api.luxgen.com npm run test:api",
    "test:api:production": "API_BASE_URL=https://luxgen-backend.netlify.app npm run test:api"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
